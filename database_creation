CREATE TABLE internship_application (
    id_no INT(11) PRIMARY KEY AUTO_INCREMENT,
    joinDate DATE NOT NULL,
    internName VARCHAR(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL,
    email VARCHAR(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL,
    phoneNumber BIGINT(10) NOT NULL,
    dob DATE NOT NULL,
    age TEXT CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL,
    department VARCHAR(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL,
    internPeriod INT(11) NOT NULL,
    lastDate TEXT CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NOT NULL
);
